<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextWithIconTextView">
        <attr name="text" format="string" />
        <attr name="iconWaiting" format="reference" />
        <attr name="iconRunning" format="reference" />
        <attr name="iconSuccess" format="reference" />
        <attr name="iconError" format="reference" />
        <attr name="status">
            <enum name="waiting" value="0"/>
            <enum name="running" value="1"/>
            <enum name="success" value="2"/>
            <enum name="error" value="3"/>
        </attr>
    </declare-styleable>
</resources>